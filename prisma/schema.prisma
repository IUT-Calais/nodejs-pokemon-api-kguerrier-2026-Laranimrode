generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Type {
  id   Int    @id @default(autoincrement())
  name String @unique
  pokemonCards PokemonCard[]
}

model PokemonCard {
  id   Int    @id @default(autoincrement())
  name String @unique
  pokedexId Int @unique
  types Type[]
  lifePoints Int 
  size Float?
  weight Float?
  imageUrl String?
}
